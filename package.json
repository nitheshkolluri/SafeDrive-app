{
  "name": "safedrive-app",
  "private": true,
  "version": "1.2.0",
  "description": "Smart navigation and rewards application promoting safer driving habits",
  "type": "module",
  "author": "SafeDrive Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nitheshkolluri/SafeDrive-app.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/safedrive/issues"
  },
  "homepage": "https://github.com/yourusername/safedrive#readme",
  "keywords": [
    "navigation",
    "driving",
    "safety",
    "rewards",
    "pwa",
    "react",
    "typescript",
    "maps",
    "geolocation"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "cap:sync": "cap sync",
    "cap:android": "cap open android",
    "cap:ios": "cap open ios",
    "docker:build": "docker build -t safedrive-app .",
    "docker:run": "docker run -p 8080:8080 -e PORT=8080 safedrive-app",
    "docker:compose": "docker-compose up",
    "docker:compose:prod": "docker-compose -f docker-compose.prod.yml up"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/geolocation": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/motion": "^6.0.0",
    "@google/genai": "^1.30.0",
    "firebase": "^10.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
